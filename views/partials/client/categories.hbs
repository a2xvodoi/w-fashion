<div class="container-fluid pt-5 border-bottom pb-5">
<div class="row">
    <div class="col-md-3 col-sm pb-5">
    <!--catagory-->
        <div class="container-fluid">
        <div class="border-bottom text-center h3 text-danger fs-md-3 fs-sm-4 hine pb-2">Danh mục sản phẩm</div>
        <div class="row">
            {{#each category}}
                <a href="/{{this.slug}}" class="col-md-12 col-3 col-sm-12 border-bottom fs-md-4 text-secondary pb-2 pt-2" style="text-decoration: none;">{{this.tenDm}}</a>
            {{else}}
            <div class="alert alert-danger text-center">Danh mục sản phẩm không tồn tại!!!</div>
            {{/each}}
        </div>
        </div>
    <!--catagory-->
    </div>
    
    <div class="col-md-9 col-sm">
    <!--carousel-->
    <div id="carouselExampleDark" class="carousel carousel-dark slide" data-bs-ride="carousel">
        <ol class="carousel-indicators">
        <li data-bs-target="#carouselExampleDark" data-bs-slide-to="0" class="active"></li>
        <li data-bs-target="#carouselExampleDark" data-bs-slide-to="1"></li>
        <li data-bs-target="#carouselExampleDark" data-bs-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
        <div class="carousel-item active" data-bs-interval="2000">
            <img src="images/pexels-photo-5246429.jpeg" class="d-block w-100 img-thumbnail" alt="...">
            <div class="carousel-caption d-none d-md-block shadow">
            <h5>First slide label</h5>
            <p>Nulla vitae elit libero, a pharetra augue mollis interdum.</p>
            </div>
        </div>
        <div class="carousel-item" data-bs-interval="2000">
            <img src="images/pexels-photo-4622994.jpeg" class="d-block w-100 img-thumbnail" alt="...">
            <div class="carousel-caption d-none d-md-block shadow">
            <h5>Second slide label</h5>
            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="images/pexels-photo-4195598.jpeg" class="d-block w-100 img-thumbnail" alt="...">
            <div class="carousel-caption d-none d-md-block shadow">
            <h5>Third slide label</h5>
            <p>Praesent commodo cursus magna, vel scelerisque nisl consectetur.</p>
            </div>
        </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleDark" role="button" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleDark" role="button" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
        </a>
    </div>
    <!--carousel-->
    </div>
</div>
</div>